{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Elastic": {
    "ServiceName": "File"
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "GroupId": "file-service",
    "AutoOffsetReset": "Earliest",
    "FileUploadTopic": "file-upload-send",
    "FilesDeleteTopic": "files-card-delete"
  },
  "MongoSettings": {
    "ConnectionString": "mongodb://admin:password@localhost:27017",
    "DatabaseName": "filesdb"
  }
}
